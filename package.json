{
  "name": "vs-code-powerschool",
  "displayName": "VS Code - PowerSchool",
  "description": "Syntax highlighting for TLIST_SQL",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "tlist_sql",
        "aliases": [
          "tlist_sql"
        ],
        "extensions": [
          ".html",
          ".htm",
          ".txt",
          ".js"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tlist_sql",
        "scopeName": "text.tlist_sql",
        "path": "./syntaxes/tlist-sql.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "tlist_sql",
        "path": "./snippets/tlist_sql.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.54.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  },
  "bugs": {
    "url": "https://github.com/bjkemp/vs-code-powerschool/issues",
    "email": "bjkemp@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bjkemp/vs-code-powerschool.git"
  },
  "homepage": "https://github.com/bjkemp/vs-code-powerschool/blob/main/README.md"
}
