{
  "comments": {
    "lineComment": "//",    
  },
  "brackets": [
    ["~[tlist_sql;", "]"],
    ["~(", ")"],
    ["[", "]"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    {
      "open": "~[tlist_sql;",
      "close": "]",
      "notIn": ["string", "comment"]
    },
    {
      "open": "~(",
      "close": ")",
      "notIn": ["comment"]
    },
    {
      "open": "[",
      "close": "]",
      "notIn": ["string", "comment"]
    },
    {
      "open": "{",
      "close": "}",
      "notIn": ["string", "comment"]
    }
  ]
}
